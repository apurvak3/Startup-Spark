<%- include('partials/header') %>

<h1>Job Postings</h1>

<% jobPostings.forEach(job => { %>
  <div>
    <h2><%= job.title %></h2>
    <p><strong>Company:</strong> <%= job.company %></p>
    <p><strong>Location:</strong> <%= job.location %></p>
    <p><%= job.description %></p>
  </div>
  <hr>
<% }) %>

<%- include('partials/footer') %>